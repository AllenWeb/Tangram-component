<script language="javascript">
var data = [{
    content: "复制",
    icon: "-176px -128px",
    title: "复制当前单元格"
}, {
    content: '复制11',
    click: function(){
        alert('复制511')
    },
    icon: "-176px -96px"
}, {
    content: '复制12',
    icon: "-192px -96px"
}, {
    content: '复制13',
    icon: "-208px -96px"
}, {
    content: '复制14',
    icon: "-224px -96px"
}, {
    content: '复制15',
    icon: "-240px -96px"
}]

var options = {
    data: data,
    value: 'test',
    skin: 'tetst',
	element:'combox',
    autoRender: true
};

var clickMenu = baidu.ui.create(baidu.ui.combox.Combox, options);

function setValue(){
	clickMenu.setValue('复制');
}
function enable(){
	clickMenu.enable(clickMenu.getBody());
}
function disable(){
	clickMenu.disable(clickMenu.getBody());
}
</script>
